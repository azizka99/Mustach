@* @model MustacheEnt.ViewModel.DetailGalleryViewModel *@
@model MustacheEnt.ViewModel.HomeViewModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
<div id="body">
    <h1><span>@Model.GalleryforDetail.FirstTitle</span></h1>
    <div>
        <img src="~/@Model.GalleryforDetail.Src" alt="">
        <div class="article">
            <h4>@Model.GalleryforDetail.SecondTitle</h4>
            <p>
                @Model.GalleryforDetail.Desc
            </p>
        </div>
    </div>
</div>